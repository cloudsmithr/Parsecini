{
  "LogSettings": {
    "LogFolder": "Logs",
    "LogFileName": "ParseciniLog.log"
  },
  "MarkdownElements": [
    {
      "name": "Header1",
      "markdownOpenSymbol": "#",
      "markdownCloseSymbol": "",
      "htmlOpenSymbol": "<h1>",
      "htmlCloseSymbol": "</h1>"
    },
    {
      "name": "Paragraph1",
      "markdownOpenSymbol": "[text]",
      "markdownCloseSymbol": "[/text]",
      "htmlOpenSymbol": "<p>",
      "htmlCloseSymbol": "</p>",
      "replaceNewlineWithBR": true
    },
    {
      "name": "Image1",
      "markdownOpenSymbol": "+",
      "markdownCloseSymbol": "",
      "htmlOpenSymbol": "<img {Content}>",
      "htmlCloseSymbol": ""
    }
  ],
  "FileSettings": {
    "MarkdownFileExtension": ".mdt",
    "TemplateFileExtension": ".tpl",
    "OutputFolder": "Output"
  }
}